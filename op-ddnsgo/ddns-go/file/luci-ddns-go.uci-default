#!/bin/sh

[ -f "/etc/config/ucitrack" ] && {
uci -q batch <<-EOF >/dev/null
	delete ucitrack.@ddns-go[-1]
	add ucitrack ddns-go
	set ucitrack.@ddns-go[-1].init=ddns-go
	commit ucitrack
EOF
}
[ -s "/etc/ddns-go/localtime" ] && mv -f /etc/ddns-go/localtime /etc/localtime
/etc/init.d/ddns-go enable
/etc/init.d/ddns-go start
rm -f /tmp/luci-indexcache* 2>/dev/null
rm -f /tmp/luci-modulecache/* 2>/dev/null  # OpenWrt 21.02
rm -f /tmp/luci-indexcache

rm -rf /tmp/luci-*
exit 0
